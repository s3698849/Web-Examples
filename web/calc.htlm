function inputTextarea() {

    var regex = /\d/;
    var textareaInput = document.getElementById('calculator-input');

    if (textareaInput.value.trim() == "0") {
        textareaInput.value = "0";
    }

}

function mode() {

    var inputDiv = document.querySelector('.input-cal-data');
    var mode = document.querySelector('.mode-button');
    var textareaInput = document.getElementById('calculator-input');

    if(inputDiv.style.display=="block"){
        mode.style.display="flex";
        textareaInput.focus
    }else{
        mode.style.display="none";
    }
   
   
}

function closeMode() {

    var mode = document.querySelector('.mode-button');
    var textareaInput = document.getElementById('calculator-input');

    mode.style.display = "none";
    textareaInput.focus();
}

function enShift() {

    var inputDiv = document.querySelector('.input-cal-data');
    var arcin = document.getElementById('arc-sin');
    var arcos = document.getElementById('arc-cos');
    var arcan = document.getElementById('arc-tan');
    var xRoot = document.getElementById('x-root');
    var textareaInput = document.getElementById('calculator-input');

    if(inputDiv.style.display=="block"){
        arcin.style.marginTop = "-30px";
        arcos.style.marginTop = "-30px";
        arcan.style.marginTop = "-30px";
        xRoot.style.marginTop = "-30px";
        textareaInput.focus();
    }else{
        arcin.style.marginTop = "0px";
        arcos.style.marginTop = "0px";
        arcan.style.marginTop = "0px";
        xRoot.style.marginTop = "0px";
    }

}

function disShift() {

    var arcin = document.getElementById('arc-sin');
    var arcos = document.getElementById('arc-cos');
    var arcan = document.getElementById('arc-tan');
    var xRoot = document.getElementById('x-root');
    var textareaInput = document.getElementById('calculator-input');

    arcin.style.marginTop = "0px";
    arcos.style.marginTop = "0px";
    arcan.style.marginTop = "0px";
    xRoot.style.marginTop = "0px";
    textareaInput.focus();

}

function deg() {

    var deg = document.querySelector('.cal-con-one-tex-two');
    var rad = document.querySelector('.cal-con-one-tex-one');
    var mode = document.querySelector('.mode-button');
    var textareaInput = document.getElementById('calculator-input');
    var inputDiv = document.querySelector('.input-cal-data');

    mode.style.display = "none";

    rad.style.display = "none";
    deg.style.display = "block";
    textareaInput.focus();

}


function rad() {

    var deg = document.querySelector('.cal-con-one-tex-two');
    var rad = document.querySelector('.cal-con-one-tex-one');
    var mode = document.querySelector('.mode-button');
    var textareaInput = document.getElementById('calculator-input');

    mode.style.display = "none";

    rad.style.display = "block";
    deg.style.display = "none";
    textareaInput.focus();

}

function on() {

    var inputDiv = document.querySelector('.input-cal-data');
    var inputBox = document.getElementById('calculator-input');
    var calResult = document.querySelector('.result');
    var arcin = document.getElementById('arc-sin');
    var arcos = document.getElementById('arc-cos');
    var arcan = document.getElementById('arc-tan');
    var xRoot = document.getElementById('x-root');
    var deg = document.querySelector('.cal-con-one-tex-two');
    var rad = document.querySelector('.cal-con-one-tex-one');
    var mode = document.querySelector('.mode-button');

    mode.style.display = "none";

    arcin.style.marginTop = "0px";
    arcos.style.marginTop = "0px";
    arcan.style.marginTop = "0px";
    xRoot.style.marginTop = "0px";

    inputDiv.style.display = "block";
    inputBox.style.background = "rgba(0,0,0,0.05)";
    calResult.style.background = "rgba(0,0,0,0.05)";
    inputBox.focus();
    inputBox.value = "0";

    if(rad.style.display=="block" || deg.style.display=="none"){
        rad.style.display = "block";
        deg.style.display = "none";
    }else{
        rad.style.display = "none";
        deg.style.display = "block";
    }
}

function off() {

    var inputDiv = document.querySelector('.input-cal-data');
    var arcin = document.getElementById('arc-sin');
    var arcos = document.getElementById('arc-cos');
    var arcan = document.getElementById('arc-tan');
    var xRoot = document.getElementById('x-root');
    var deg = document.querySelector('.cal-con-one-tex-two');
    var rad = document.querySelector('.cal-con-one-tex-one');
    var mode = document.querySelector('.mode-button');

    mode.style.display = "none";

    arcin.style.marginTop = "0px";
    arcos.style.marginTop = "0px";
    arcan.style.marginTop = "0px";
    xRoot.style.marginTop = "0px";
    document.getElementById('mode-btn').disabled;
    inputDiv.style.display = "none";
    rad.style.display = "none";
    deg.style.display = "none";

}

