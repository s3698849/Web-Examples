}

#shift-btn{
    margin-left: 3%;
}

.cal-header{
    display: flex;
    width: 100%;
    align-items: center;
}

.mode{
    display: flex;
    width: 50%;
    justify-content: flex-end;
}

#close-mode{
    color: red;
    margin-left: 3%;
    font-size: 20px;
    cursor: pointer;
}

#close-mode:hover{
    color: white;
}

.mode-button{
    width: 70%;
    display: none;
    background-color: goldenrod;
    padding-top: 5px;
    padding-bottom: 5px;
    justify-content: center;
    border-radius: 5px;
