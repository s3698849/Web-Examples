.logo{
    width: 10%;
    height: 35px;
    margin-top: 10px;
}

.logo img{
    width: 100%;
    height: 100%;
}

.calculator-contents{
    width: 100%;
    margin-top: 20px;
    border: 1px solid black;
    background: white;
    
}

.input-cal-data{
    width: 100%;
    display: none;
}

.result{
    width: 100%;
    height: 30px;
    text-align: right;
    font-size: 25px;
}

.calculator-contents-one{
    width: 100%;
    height: 100px;
    border-bottom: 1px black solid;
}

#calculator-input{
    width: 100%;
    height: 70px;
    text-align: right;
    padding: 5px;
    border-bottom: none;
    font-size: 18px;
}

#calculator-input:focus{
    border: none;
}

.calculator-contents-two{
    margin-top: 20px;
    width: 100%;
}

.calculator-content-two-content-one{
    width: 100%;
    display: flex;
}

.calculator-content-two-content-one-btn-one{
   width: 50%; 
