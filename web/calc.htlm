            .calculator{
    border: 2px solid black;
    border-radius: 10px;
    width: 10.7cm;
    padding-left: 20px;
    padding-right: 20px;
    margin-right: auto;
    margin-left: auto;
    padding-bottom: 20px;
    background: rgba(0,0,0,0.2);
    box-shadow: 10px 10px black;
    min-width: 10.7cm;
    max-width: 10.7cm;
    
